    "@nestjs/swagger": "^7.4.0",
    "@types/mongodb": "^4.0.7",
    "@types/multer": "^1.4.11",
    "axios": "^1.7.2",
    "cache-manager": "^5.7.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "mongodb": "^6.8.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.4",
    "openfoodfacts-nodejs": "github:openfoodfacts/openfoodfacts-nodejs",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "save": "^2.9.0",
